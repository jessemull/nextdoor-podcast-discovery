# Nextdoor Scraper Environment Variables
# Copy this file to .env and fill in your values

# Nextdoor credentials

NEXTDOOR_EMAIL=your-email@example.com
NEXTDOOR_PASSWORD=your-password

# Supabase (use service key for full access)

SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_SERVICE_KEY=eyJ...

# Encryption key for session cookies (generate with: python scripts/generate-encryption-key.py)

SESSION_ENCRYPTION_KEY=your-fernet-key

# LLM API keys

ANTHROPIC_API_KEY=sk-ant-...
OPENAI_API_KEY=sk-...
